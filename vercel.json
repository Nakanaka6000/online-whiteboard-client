{
  "version": 2,
  "builds": [
    {
      "src": "client/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "client/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "client/script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/client/index.html"
    },
    {
      "src": "/style.css",
      "dest": "/client/style.css"
    },
    {
      "src": "/script.js",
      "dest": "/client/script.js"
    }
  ]
}
